
namespace FBS.Common;

/* Optional scalars */

table OptionalInt16 {
  value:int16;
}

/* Common custom types */

table StringString {
  key:string (required);
  value:string (required);
}

table StringUint8 {
  key:string (required);
  value:uint8;
}

table Uint16String {
  key:uint16;
  value:string (required);
}

table Uint32String {
  key:uint32;
  value:string (required);
}

table StringStringArray {
  key:string (required);
  values:[string];
}

